{
  "defaultSeverity": "error",
  "extends": [
    "dtslint/dtslint.json"
  ],
  "jsRules": {},
  "rules": {
    "file-name-casing": false,
    "no-unnecessary-generics": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "strict-export-declare-modifiers": false
  }
}
